{
  "name": "@perttu/app-store-scraper",
  "version": "3.0.0",
  "description": "Modern TypeScript library to scrape application data from the iTunes/Mac App Store",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "test:integration": "RUN_INTEGRATION_TESTS=1 vitest run",
    "test:coverage": "vitest run --coverage",
    "example": "tsx examples/all-methods.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plahteenlahti/app-store-scraper.git"
  },
  "bugs": {
    "url": "https://github.com/plahteenlahti/app-store-scraper/issues"
  },
  "homepage": "https://github.com/plahteenlahti/app-store-scraper#readme",
  "keywords": [
    "app-store",
    "itunes",
    "scraper",
    "ios",
    "ipad",
    "mac",
    "apple",
    "typescript",
    "app-data",
    "reviews",
    "ratings"
  ],
  "author": "Perttu LÃ¤hteenlahti",
  "license": "MIT",
  "publishConfig": { "access": "public" },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.2",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "fast-xml-parser": "^4.3.2",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
